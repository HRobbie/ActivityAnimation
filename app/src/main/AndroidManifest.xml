<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hrobbie.activityanimation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CommonActivity" />
        <activity android:name=".ExplodeActivity" />
        <activity android:name=".FadeActivity" />
        <activity android:name=".ShareActivity" />
        <activity android:name=".BlueActivity" />
        <activity android:name=".CustomShareActivity" />
        <activity android:name=".CircleActivity"></activity>
    </application>

</manifest>